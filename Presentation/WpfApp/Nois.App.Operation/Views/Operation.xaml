                                                        <DataTrigger.EnterActions>
                                                            <BeginStoryboard >
                                                                <Storyboard>
                                                                    <DoubleAnimation Storyboard.TargetProperty="(Canvas.Bottom)" From="-15" To="-5" Duration="0:0:1" RepeatBehavior="Forever"/>
                                                                </Storyboard>
                                                            </BeginStoryboard>
                                                        </DataTrigger.EnterActions>
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Image.Style>
                                    </Image>
                                </Canvas>
                            </Grid>
                        </Border>
                        <Border Grid.Column="9" Margin="0 0 20 0" CornerRadius="10 10 0 0">
                            <Border.Style>
                                <Style TargetType="Border">
                                    <Setter Property="Background" Value="#039be5"/>
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding Chute3.IsActive}" Value="False">
                                            <Setter Property="Background" Value="Gray" />
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </Border.Style>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <TextBlock Text="{Binding Chute3.Name}" VerticalAlignment="Top" HorizontalAlignment="Center" Margin="5" Foreground="White"/>
                                <Canvas Grid.Row="1" HorizontalAlignment="Center" Margin="0 5 15 20" VerticalAlignment="Bottom">
                                    <Image Grid.Row="1" Width="15" Height="15">
                                        <Image.Style>
                                            <Style TargetType="Image">
                                                <Setter Property="Source" Value="/Nois.App.Operation;component/Resources/Images/arrow_up.png"/>
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding Chute3.IsActive}" Value="False">
                                                        <Setter Property="Source" Value="/Nois.App.Operation;component/Resources/Images/stop.png" />
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding Chute3.IsActive}" Value="True">
                                                        <DataTrigger.EnterActions>
                                                            <BeginStoryboard >
                                                                <Storyboard>
                                                                    <DoubleAnimation Storyboard.TargetProperty="(Canvas.Bottom)" From="-15" To="-5" Duration="0:0:1" RepeatBehavior="Forever"/>
                                                                </Storyboard>
                                                            </BeginStoryboard>
                                                        </DataTrigger.EnterActions>
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Image.Style>
                                    </Image>
                                </Canvas>
                            </Grid>
                        </Border>
                        <Border Grid.Column="8" Margin="0 0 20 0" CornerRadius="10 10 0 0">
                            <Border.Style>
                                <Style TargetType="Border">
                                    <Setter Property="Background" Value="#039be5"/>
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding Chute1.IsActive}" Value="False">
                                            <Setter Property="Background" Value="Gray" />
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </Border.Style>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <TextBlock Text="{Binding Chute1.Name}" VerticalAlignment="Top" HorizontalAlignment="Center" Margin="5" Foreground="White"/>
                                <Canvas Grid.Row="1" HorizontalAlignment="Center" Margin="0 5 15 20" VerticalAlignment="Bottom">
                                    <Image Width="15" Height="15">
                                        <Image.Style>
                                            <Style TargetType="Image">
                                                <Setter Property="Source" Value="/Nois.App.Operation;component/Resources/Images/arrow_up.png"/>
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding Chute1.IsActive}" Value="False">
                                                        <Setter Property="Source" Value="/Nois.App.Operation;component/Resources/Images/stop.png" />
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding Chute1.IsActive}" Value="True">
                                                        <DataTrigger.EnterActions>
                                                            <BeginStoryboard >
                                                                <Storyboard>
                                                                    <DoubleAnimation Storyboard.TargetProperty="(Canvas.Bottom)" From="-15" To="-5" Duration="0:0:1" RepeatBehavior="Forever"/>
                                                                </Storyboard>
                                                            </BeginStoryboard>
                                                        </DataTrigger.EnterActions>
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Image.Style>
                                    </Image>
                                </Canvas>
                            </Grid>
                        </Border>
                        <Border Grid.Row="1" BorderBrush="#f44336" BorderThickness="1" Grid.Column="5" Margin="0 -25" CornerRadius="10">
                            <Border.Style>
                                <Style TargetType="Border">
                                    <Setter Property="Background" Value="#f44336"/>
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding DV6}" Value="0">
                                            <Setter Property="Background" Value="Gray" />
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </Border.Style>
                            <TextBlock Text="Camera Scanner" TextWrapping="Wrap" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
                        </Border>
                        <Grid Grid.Row="2" Grid.ColumnSpan="18" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="5">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <Grid Margin="10 5">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Text="{Binding Sender}" Margin="5"/>
                                <Ellipse Grid.Column="1" Margin="5" Width="10" Height="10" VerticalAlignment="Center" Fill="{Binding SenderStatus}"/>
                            </Grid>
                            <Grid Margin="10 5" Grid.Column="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Text="{Binding Receiver}" Margin="5"/>
                                <Ellipse Grid.Column="1" Margin="5" Width="10" Height="10" VerticalAlignment="Center" Fill="{Binding ReceiverStatus}"/>
                            </Grid>
                            <Grid Margin="10 5" Grid.Column="2">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Text="{Binding Status}" Margin="5"/>
                                <Ellipse Grid.Column="1" Margin="5" Width="10" Height="10" VerticalAlignment="Center" Fill="{Binding StatusStatus}"/>
                            </Grid>
                            <Grid Margin="10 5" Grid.Column="3">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Text="{Binding Update}" Margin="5"/>
                                <Ellipse Grid.Column="1" Margin="5" Width="10" Height="10" VerticalAlignment="Center" Fill="{Binding UpdateStatus}"/>
                            </Grid>
                            <Grid Margin="10 5" Grid.Column="4">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Text="{Binding Heartbeat}" Margin="5"/>
                                <Ellipse Grid.Column="1" Margin="5" Width="10" Height="10" VerticalAlignment="Center" Fill="{Binding HeartbeatStatus}"/>
                            </Grid>
                        </Grid>
                        <Grid HorizontalAlignment="Left" VerticalAlignment="Top" Margin="5">
                            <Button Margin="10" FontSize="16" Height="Auto" Width="Auto" Command="{Binding RefreshCommand}">
                                <materialDesign:PackIcon Kind="Autorenew" />
                            </Button>
                        </Grid>
                    </Grid>
                </ScrollViewer>
            </Expander>
        </materialDesign:Card>
        <Grid Grid.Row="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <materialDesign:Card Margin="5">
                <Expander ExpandDirection="Right">
                    <Expander.Header>
                        <TextBlock Text="Map"/>
                    </Expander.Header>
                    <Expander.Content>
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <GroupBox Style="{DynamicResource MaterialDesignGroupBox}" Margin="10">
                                <GroupBox.Header>
                                    <DockPanel>
                                        <materialDesign:PackIcon Kind="Sitemap" DockPanel.Dock="Left" VerticalAlignment="Center" Width="20" Height="20" Foreground="White"/>
                                        <TextBlock DockPanel.Dock="Right" Text="Mapping" Foreground="White" FontWeight="Bold" Margin="10 0 0 0" FontSize="16"/>
                                    </DockPanel>
                                </GroupBox.Header>
                                <Grid>
                                    <Grid Margin="0 10 0 30">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition/>
                                            <ColumnDefinition Width="Auto"/>
                                        </Grid.ColumnDefinitions>
                                        <ComboBox ItemsSource="{Binding Mappings}" FontSize="16" Margin="10" DisplayMemberPath="Name" Text="{Binding SelectedMapping.Name}" SelectedItem="{Binding SelectedMapping}" materialDesign:HintAssist.Hint="Select Mapping" ScrollViewer.VerticalScrollBarVisibility="Auto">
                                            <i:Interaction.Triggers>
                                                <i:EventTrigger EventName="DropDownOpened">
                                                    <i:InvokeCommandAction Command="{Binding ComboboxCommand}"/>
                                                </i:EventTrigger>
                                            </i:Interaction.Triggers>
                                        </ComboBox>
                                        <Button Grid.Column="1" Content="Apply" Margin="10 10 0 10" FontSize="16" Height="Auto" Command="{Binding ApplyCommand}"/>
                                    </Grid>
                                </Grid>
                            </GroupBox>
                            <GroupBox Grid.Row="1" Style="{DynamicResource MaterialDesignGroupBox}" Margin="10 0 10 10" VerticalAlignment="Top">
                                <GroupBox.Header>
                                    <Grid>
                                        <DockPanel HorizontalAlignment="Left" VerticalAlignment="Center">
                                            <materialDesign:PackIcon Kind="InformationOutline" DockPanel.Dock="Left" VerticalAlignment="Center" Width="20" Height="20" Foreground="White"/>
                                            <TextBlock DockPanel.Dock="Right" Text="Summary Chute" Foreground="White" FontWeight="Bold" Margin="10 0 0 0" FontSize="16"/>
                                        </DockPanel>
                                        <Button Content="Reset" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="0 -2" Command="{Binding ResetCommand}" BorderBrush="#f44336" Background="#f44336"/>
                                    </Grid>
                                </GroupBox.Header>
                                <ListView ItemsSource="{Binding MappingModels}" Margin="5" FontSize="14">
                                    <ListView.View>
                                        <GridView>
                                            <GridView.ColumnHeaderContainerStyle>
                                                <Style TargetType="GridViewColumnHeader">
                                                    <Setter Property="BorderThickness" Value="0.5"/>
                                                    <Setter Property="Padding" Value="15"/>
                                                    <Setter Property="HorizontalContentAlignment" Value="Left"/>
                                                    <Setter Property="Foreground" Value="DimGray"/>
                                                    <Setter Property="FontWeight" Value="DemiBold"/>
                                                </Style>
                                            </GridView.ColumnHeaderContainerStyle>
                                            <GridViewColumn DisplayMemberBinding="{Binding Chute}" Header="Chute" Width="Auto"/>
                                            <GridViewColumn Header="Destination" Width="Auto">
                                                <GridViewColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <ItemsControl ItemsSource="{Binding Destinations}">
                                                            <ItemsControl.ItemsPanel>
                                                                <ItemsPanelTemplate>
                                                                    <StackPanel Orientation="Horizontal"/>
                                                                </ItemsPanelTemplate>
                                                            </ItemsControl.ItemsPanel>
                                                            <ItemsControl.ItemTemplate>
                                                                <DataTemplate>
                                                                    <StackPanel Orientation="Horizontal">
                                                                        <TextBlock Margin="5 0 0 0" Text="{Binding Name}"/>
                                                                        <TextBlock Text=","/>
                                                                    </StackPanel>
                                                                </DataTemplate>
                                                            </ItemsControl.ItemTemplate>
                                                        </ItemsControl>
                                                    </DataTemplate>
                                                </GridViewColumn.CellTemplate>
                                            </GridViewColumn>
                                            <GridViewColumn Header="Quantity" DisplayMemberBinding="{Binding Quantity}" Width="Auto"/>
                                        </GridView>
                                    </ListView.View>
                                </ListView>
                            </GroupBox>
                        </Grid>
                    </Expander.Content>
                </Expander>
            </materialDesign:Card>
            <GroupBox Grid.Column="1" Style="{DynamicResource MaterialDesignCardGroupBox}" Margin="0 10 10 20">
                <GroupBox.Header>
                    <DockPanel>
                        <materialDesign:PackIcon Kind="History" DockPanel.Dock="Left" VerticalAlignment="Center" Width="20" Height="20" Foreground="White"/>
                        <TextBlock Text="Sorting Log Data" Foreground="White" FontWeight="Bold" Margin="10 0 0 0" FontSize="16" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                        <ComboBox ItemsSource="{Binding Columns}" materialDesign:HintAssist.Hint="{Binding ColumnText}" FontSize="16" Margin="0" ScrollViewer.VerticalScrollBarVisibility="Auto" HorizontalAlignment="Right" Width="170" Background="White">
                            <ComboBox.ItemTemplate>
                                <DataTemplate>
                                    <CheckBox Content="{Binding Title}" IsChecked="{Binding IsSelected, Mode=TwoWay}" 
                                          Tag="{RelativeSource FindAncestor, AncestorType={x:Type ComboBox}}"
                                          Command="{Binding CheckboxCheck}" CommandParameter="{Binding}"/>
                                </DataTemplate>
                            </ComboBox.ItemTemplate>
                        </ComboBox>
                    </DockPanel>
                </GroupBox.Header>
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="1*"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <ListView ItemsSource="{Binding ScannedLogs}" Margin="5" FontSize="16" ScrollViewer.HorizontalScrollBarVisibility="Visible" ScrollViewer.CanContentScroll="True">
                        <ListView.View>
                            <GridView>
                                <GridView.ColumnHeaderContainerStyle>
                                    <Style TargetType="GridViewColumnHeader">
                                        <Setter Property="BorderThickness" Value="0.5"/>
                                        <Setter Property="Padding" Value="15"/>
                                        <Setter Property="HorizontalContentAlignment" Value="Left"/>
                                        <Setter Property="Foreground" Value="DimGray"/>
                                        <Setter Property="FontWeight" Value="DemiBold"/>
                                    </Style>
                                </GridView.ColumnHeaderContainerStyle>
                                <GridViewColumn Width="200" DisplayMemberBinding="{Binding Barcode1}" Header="Barcode 1" framework:GridViewColumnVisibilityManager.IsVisible="{Binding Path=Columns[1].IsSelected}"/>
                                <GridViewColumn Width="200" DisplayMemberBinding="{Binding Barcode2}" Header="Barcode 2" framework:GridViewColumnVisibilityManager.IsVisible="{Binding Path=Columns[2].IsSelected}"/>
                                <GridViewColumn Width="200" DisplayMemberBinding="{Binding Barcode3}" Header="Barcode 3" framework:GridViewColumnVisibilityManager.IsVisible="{Binding Path=Columns[3].IsSelected}"/>
                                <GridViewColumn Width="200" DisplayMemberBinding="{Binding SortedCode}" Header="SortedCode" framework:GridViewColumnVisibilityManager.IsVisible="{Binding Path=Columns[4].IsSelected}"/>
                                <GridViewColumn Width="Auto" DisplayMemberBinding="{Binding Setting}" Header="Setting" framework:GridViewColumnVisibilityManager.IsVisible="{Binding Path=Columns[5].IsSelected}"/>
                                <GridViewColumn Width="Auto" DisplayMemberBinding="{Binding SettingName}" Header="Setting Name" framework:GridViewColumnVisibilityManager.IsVisible="{Binding Path=Columns[6].IsSelected}"/>
                                <GridViewColumn Width="Auto" DisplayMemberBinding="{Binding Mode}" Header="Check Duplicate" framework:GridViewColumnVisibilityManager.IsVisible="{Binding Path=Columns[7].IsSelected}"/>
                                <GridViewColumn Width="Auto" DisplayMemberBinding="{Binding Dimension}" Header="Dimension (LxWxH mm)" framework:GridViewColumnVisibilityManager.IsVisible="{Binding Path=Columns[8].IsSelected}"/>
                                <GridViewColumn Width="Auto" DisplayMemberBinding="{Binding Weight}" Header="Weight (gram)" framework:GridViewColumnVisibilityManager.IsVisible="{Binding Path=Columns[9].IsSelected}"/>
                                <GridViewColumn Width="Auto" DisplayMemberBinding="{Binding Destination}" Header="Destination" framework:GridViewColumnVisibilityManager.IsVisible="{Binding Path=Columns[10].IsSelected}"/>
                                <GridViewColumn Width="Auto" DisplayMemberBinding="{Binding Chute}" Header="Chute" framework:GridViewColumnVisibilityManager.IsVisible="{Binding Path=Columns[11].IsSelected}"/>
                                <GridViewColumn Width="Auto" DisplayMemberBinding="{Binding Time}" Header="Time" framework:GridViewColumnVisibilityManager.IsVisible="{Binding Path=Columns[12].IsSelected}"/>
                                <GridViewColumn Width="400" DisplayMemberBinding="{Binding Remark}" Header="Remark" framework:GridViewColumnVisibilityManager.IsVisible="{Binding Path=Columns[13].IsSelected}"/>
                            </GridView>
                        </ListView.View>
                    </ListView>
                    <Views:Pagger x:Name="pagger" Grid.Row="1" HorizontalAlignment="Center" Margin="5"/>
                </Grid>
            </GroupBox>
        </Grid>
    </Grid>
</UserControl>
